{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "NodeNext",
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "outDir": "dist",
        "rootDir": "src",
        "baseUrl": "src",
        "paths": {
            "@/*": ["*"]
        }
    },
    "include": []
}